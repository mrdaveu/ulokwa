# Neon REST API Configuration
# Copy this file to .env and fill in your values

# Your Neon REST API endpoint
NEON_REST_API_URL=https://ep-frosty-fire-ae0rgj8a.apirest.c-2.us-east-2.aws.neon.tech/neondb/rest/v1

# Your Neon API key (get from Neon Console → Account Settings → API Keys)
NEON_API_KEY=your_api_key_here

# Stack Auth credentials (if using authentication)
STACK_PROJECT_ID=your_stack_project_id
STACK_PUBLISHABLE_CLIENT_KEY=your_publishable_key
STACK_SECRET_SERVER_KEY=your_secret_key

# Database owner connection string (alternative to REST API)
DATABASE_URL=your_postgresql_connection_string
